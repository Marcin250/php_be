{
	"autoload": {
		"classmap": [
		],
        "psr-4": {
            "App\\": "app/",
			"Config\\": "config/"
        }
	},
	"require": {
		"vlucas/phpdotenv": "^3.3",
		"google/apiclient": "^2.0"
	},
	"post-install-cmd": [
		"php artisan clear-compiled",
		"php artisan optimize",
		"chmod -R 777 public/"
	],
	"require-dev": {
		"phpunit/phpunit": "8"
	}
}
